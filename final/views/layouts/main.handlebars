<!DOCTYPE html>
<html lang="en" data-bs-theme="light">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {{!-- Import css styles --}}
    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/owl/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="/owl/assets/owl.theme.default.min.css"> 
    <link rel="stylesheet" href="/css/app.css">  
   {{!-- Overrides go last --}}
</head>
<body class="pattern-bg">
   {{!-- Include a partial --}}
   {{>nav siteName="GamesDotCom"}}
    {{!-- Default placeholder for handlebars --}}
   <div class="container">
      {{{body}}}
    </div>
    <!-- jQuery should always be the first script -->
    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
    <script src="/js/bootstrap.bundle.js"></script>
    <script src="/owl/owl.carousel.min.js"></script>

<script>
$(function(){
    //Slideshow
   $('.owl-item_slideshow').owlCarousel({
    loop:true,
    margin:0,
    autoWidth:true,
    autoplay:true,
    autoplaySpeed:1000,
    autoplayTimeout:4000,
    dotsSpeed:false,
    mouseDrag:false,
    touchDrag:false,
    pullDrag:false,
    freeDrag:false,
    dots:false,
    });
    //packages
    $('.owl-featured_products').owlCarousel({
    loop:false,
    margin:10,
    nav:true,
    });
});
</script>

</body>
</html>